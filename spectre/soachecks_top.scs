// Metal resistors part

section rm_10hv_soa_shared // shared SOA code for all metal resistors
parameters
+ weff_soa = w + dw_um // in um; used for resistance estimate
+ reff_soa = rsh_soa * leff_soa / weff_soa
parameters // Idc, IPeak, Irms
+ vmax_rm_dc = jdc_rm * weff1_soa * reff_soa
+ vmax_rm_peak = jpeak_rm * weff1_soa * reff_soa
+ irms_max = 1.0e-3 * sqrt(jdc_rms * weff1_soa * (weff1_soa + dw_rms)) // in Ampere 
model irms_rm ovcheck
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ vlow=-1.0  vhigh=dtmax_rm branch1="V(dt)" message1="Irms: self heating exceeds 5 degC."
model idc_rm ovcheck
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ vlow=-vmax_rm_dc vhigh=vmax_rm_dc branch1="V(d,s)"
+ message1="Max. dc-current exceeded (violations are allowed for limited time)." 
model ipeak_rm ovcheck
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ vlow=-vmax_rm_peak vhigh=vmax_rm_peak branch1="V(d,s)"
+ message1="Max. peak current exceeded (violations <100ns may be allowed)." 
sh_dummy (d s dt) shmonitor_nofeedback
+ r=reff_soa
+ irms_max=irms_max
+ dtmax=dtmax_rm
+ theat=theat
soacheck_rms (dt 0) irms_rm
soacheck_dc (d s) idc_rm
soacheck_peak (d s) ipeak_rm
endsection rm_10hv_soa_shared


// Poly resistors part

section rpoly_10hv_voltage_soa_shared // shared voltage SOA code for all poly resistors
model ovcheck_1 ovcheck6
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ vlow1=-45         vhigh1=45           branch1="V(d,well)"
+ vlow2=-45         vhigh2=45           branch2="V(s,well)"
+ vlow3=-70         vhigh3=120          branch3="V(well,sub)"
soacheck1 (d well s well well sub 0 0 0 0 0 0) ovcheck_1
endsection rpoly_10hv_voltage_soa_shared

section rpoly_10hv_current_soa_shared // shared current SOA code for all poly resistors
model ovcheck_2 parcheckva3
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ param="ibody"
+ plow=-jmax_poly   phigh=jmax_poly     message1="Max. peak current exceeded."
model ovcheck_3 parcheckva3
+ tmin=100n tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ param="ibody"
+ plow=-jmax_poly_100ns phigh=jmax_poly_100ns message1="Max. dc current exceeded (violations <100ns may be allowed)."
model ovcheck_4 parcheckva3
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac3
+ param="ibody"
+ plow=-jmax_poly_sh   phigh=jmax_poly_sh     message1="Self-heating current exceeded."
endsection rpoly_10hv_current_soa_shared

section rpoly_10hv_current_simple_soa_shared // shared current SOA code for all simple poly resistors
parameters
+ rsh_soa         = rsh0
+ leff_soa        = l + dl_um
+ weff_soa        = w + dw_um
+ reff_soa  = rsh_soa * leff_soa / weff_soa
parameters // Idc, IPeak
+ vmax_poly_dc   = jmax_poly * reff_soa
+ vmax_poly_peak = jmax_poly_100ns * reff_soa
+ vmax_poly_sh   = jmax_poly_sh * reff_soa
model idc_poly ovcheck
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ vlow=-vmax_poly_dc vhigh=vmax_poly_dc branch1="V(d,s)"
+ message1="Max. current exceeded." 
model ipeak_poly ovcheck
+ tmin=100n tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac0
+ vlow=-vmax_poly_peak vhigh=vmax_poly_peak branch1="V(d,s)"
+ message1="Max. current exceeded (violations <100ns may be allowed)." 
model ish_poly ovcheck
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac3
+ vlow=-vmax_poly_sh vhigh=vmax_poly_sh branch1="V(d,s)"
+ message1="Self-heating current exceeded." 
soacheck_dc (d s) idc_poly
soacheck_peak (d s) ipeak_poly
soacheck_sh (d s) ish_poly
endsection rpoly_10hv_current_simple_soa_shared



//HCI-TDDB part

section hci_tddb_atype_soa_shared
parameters soa_hcitddb_vdswc = soa_hcitddb_vdsref + soa_hcitddb_wfac * ln(1e-6 * w / soa_hcitddb_wref) + soa_hcitddb_tfac * (1 / (temp + tcelsius0) - 1 / soa_hcitddb_tref)
model hcitddb ovcheckva_ldmos_hci_tddb
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ message1="HCI-TDDB risk; use Life Time Estimator for accurate assessment."
+ tmaxfrac=tmaxfrac0
+ a_high=soa_hcitddb_a
+ b=soa_hcitddb_b
+ vds_wc=soa_hcitddb_vdswc
+ vgs_wc=soa_hcitddb_vgswc
+ branch1="V(d,s)"
+ device=device
soacheck_hcitddb (d g s) hcitddb device=device
endsection hci_tddb_atype_soa_shared

section hci_tddb_btype_soa_shared
parameters 
+ soa_hcitddb_woffset    = soa_hcitddb_wfac * ln(1e-6 * w / soa_hcitddb_wref)
+ soa_hcitddb_b0_eff     = soa_hcitddb_b0 + soa_hcitddb_woffset
+ soa_hcitddb_b1_eff     = soa_hcitddb_b1 + soa_hcitddb_woffset
+ soa_hcitddb_b2_eff     = soa_hcitddb_b2 + soa_hcitddb_woffset
+ soa_hcitddb_b3_eff     = soa_hcitddb_b3 + soa_hcitddb_woffset
+ soa_hcitddb_b4_eff     = soa_hcitddb_b4 + soa_hcitddb_woffset
model hcitddb ovcheckva_pwl
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ message1="HCI-TDDB risk; use Life Time Estimator for accurate assessment."
+ branch1="V(s,d)"
+ tmaxfrac=tmaxfrac0
+ device=device
+ vlim1=-soa_hcitddb_b0_eff   vc1=-soa_hcitddb_p1
+ vlim2=-soa_hcitddb_b1_eff   vc2=-soa_hcitddb_p2
+ vlim3=-soa_hcitddb_b2_eff   vc3=-soa_hcitddb_p3
+ vlim4=-soa_hcitddb_b3_eff   vc4=-soa_hcitddb_p4
+ vlim5=-soa_hcitddb_b4_eff   vc5=-soa_hcitddb_p5
soacheck_hcitddb (s d s g) hcitddb device=device
endsection hci_tddb_btype_soa_shared


