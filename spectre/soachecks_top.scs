simulator lang=spectre

section base

ahdl_include "./veriloga/ovcheck_mos_alt.va"
ahdl_include "./veriloga/ovcheck_pwl_alt.va"
ahdl_include "./veriloga/ovcheck_ldmos_hci_tddb_alt.va"
ahdl_include "./veriloga/parcheck3.va"
ahdl_include "./veriloga/selfheating_monitor_nofeedback.va"

parameters
// global SOA parameters
+ global_tmin      = 0    // Minimum time outside of SOA before a warning is issued
+ global_tdelay    = 0    // Time after start of transient that checking is activated
+ global_vballmsg  = 1.0  // All SOA messages if set to 1 (otherwise only give an overview at the end)
+ global_stop      = 0    // Stop simulation on SOA violation when STOP==1
+ tcelsius0        = 273.15

// duration limits
+ tmaxfrac0 = 0
+ tmaxfrac1 = 0.01   //  1%
+ tmaxfrac2 = 0.10   // 10%
+ tmaxfrac3 = -1     // review

// MOS junctions
+ tref_soa        = 25
+ ap_fwd_ref      = 0.9943      // limit at tref_soa
+ ap_fwd_T        = -0.0006    // -1.0mV/degC
+ ap_fwd          = ap_fwd_ref + ap_fwd_T * (temp - tref_soa)
+ ap_no_check     = 999.00    // high value, for branches that are not monitored
+ ap_gc_lv        = 1.65      // maximum oxide voltage in LV devices

// HV devices
+ ap_gc_hv        = 5.5  // maximum oxide voltage in HV devices
+ ap_fwd_hv       = 1.2  // maximum forward junction voltage in HV devices
+ ap_gc_lv_oxrisk = 1.32 // maximum oxide voltage in HV devices for oxrisk check
+ ap_gc_hv_oxrisk = 4.4  // maximum oxide voltage in HV devices for oxrisk check

// metal resistors
+ dtmax_rm        = 5    // maximum T-rise in degC
+ theat           = 1e-7 // thermal time constant (in seconds)

endsection base


// OXrisk part

section soacheck_mos_core_oxrisk_shared
model ovcheck_mos_core_oxrisk ovcheck6
+ tmin=global_tmin tdelay=global_tdelay vballmsg=global_vballmsg stop=global_stop
+ tmaxfrac=tmaxfrac3
+ vlow1=-1.32       vhigh1=1.32           branch1="V(g,b)"      message1="Vgb_OXrisk"
+ vlow2=-1.32       vhigh2=1.32           branch2="V(g,s)"      message2="Vgs_OXrisk"       
+ vlow3=-1.32       vhigh3=1.32           branch3="V(g,d)"      message3="Vgd_OXrisk"       
endsection soacheck_mos_core_oxrisk_shared

